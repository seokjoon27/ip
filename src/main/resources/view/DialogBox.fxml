<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/17"
         xmlns:fx="http://javafx.com/fxml/1"
         alignment="TOP_RIGHT" prefWidth="400.0" maxWidth="1.7976931348623157E308"
         maxHeight="1.7976931348623157E308">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity"/>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true"/>
    </children>
    <padding>
        <Insets top="15.0" right="5.0" bottom="15.0" left="5.0"/>
    </padding>
</fx:root>